<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.gragas.subscribe.mapper.SubscribeOrderMapper">
    <resultMap id="subscribeFileResult" type="com.green.gragas.subscribe.dto.SubscribeOrder">
        <result property="SO_NUM" column="SO_NUM"/>
        <result property="siNum" column="SI_NUM"/>
        <result property="sfType" column="USER_ID"/>
        <result property="sfRoot" column="SO_NAME"/>
        <result property="sfRName" column="SO_TEL"/>
        <result property="sfOName" column="SO_ADDR"/>
        <result property="sfOName" column="SO_ADDR_DE"/>
        <result property="sfOName" column="SO_MEMO"/>
        <result property="sfOName" column="SO_STATUS"/>

    </resultMap>
    <insert id="insertSubscribeFile" parameterType="subscribeFile">
        INSERT INTO subscribe_file (si_num, sf_type, sf_root, sf_r_name, sf_o_name)
        VALUES (#{siNum}, #{sfType}, #{sfRoot}, #{sfRName}, #{sfOName})
    </insert>
</mapper>