<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.gragas.subscribe.mapper.SubscribeFileMapper">
    <resultMap id="subscribeFileResult" type="com.green.gragas.subscribe.dto.SubscribeFile">
        <result property="sfId" column="SF_ID"/>
        <result property="siNum" column="SI_NUM"/>
        <result property="sfType" column="SF_TYPE"/>
        <result property="sfRoot" column="SF_ROOT"/>
        <result property="sfRName" column="SF_R_NAME"/>
        <result property="sfOName" column="SF_O_NAME"/>

    </resultMap>
    <insert id="insertSubscribeFile" parameterType="subscribeFile">
        INSERT INTO subscribe_file (si_num, sf_type, sf_root, sf_r_name, sf_o_name)
        VALUES (#{siNum}, #{sfType}, #{sfRoot}, #{sfRName}, #{sfOName})
    </insert>
</mapper>